<h3>Active Calls (<%=@call_count%>)</h3>

<% @call_list.each do |call| %>

	<script>
		$(function() {
			$( "#C<%=call.sid%>" ).draggable({ revert: true });
			$( "#dropintowaitingroom" ).droppable({
    			drop: handleDropEventIntoWaitingRoom
  			});
		});
	</script>
	
	<div id="C<%=call.sid%>" class="ui-widget-content">
		<div class="btn primary_phone" style="width: 150px">
			<img src="redx.png" />
			<p><%= get_caller_name(call) %></p>
			<p>From:<%= call.from %></p>
			<p>To:<%= call.to %></p>
		</div>
	</div>

<% end %>
