<h3>Waiting Room (<%=@wait_count%>)</h3>       

<% @waiting_participants.each do |call| %>
	<button class="btn primary_phone">
		<% if call.caller_name.nil? or call.caller_name.empty? %>
			<p>Uknown Caller</p>
		<% else %>
			<p>call.caller_name.to_s.capitalize </p>
		<% end %>
		<p>From:<%= call.from %></p>
		<p>To:<%= call.to %></p>
	</button>
<% end %>
